PROCEDURE swap(VAR xp, VAR yp : INTEGER)
VAR
   tmp : INTEGER;
BEGIN
  tmp := xp;
  xp := yp;
  yp := tmp;
END

PROCEDURE insertion_sort(VAR tab: ARRAY_OF INTEGER)
VAR
    i,j,n : INTEGER
BEGIN
    n:= tab.length;
    FOR i FROM 1 TO n-1 STEP 1 DO
    j := i
        WHILE (j > 0 and tab[j-1] > tab[j]) DO
            swap(tab[j], tab[j-1])
            j := j - 1
        END_WHILE
   END_FOR
END